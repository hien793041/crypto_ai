												<a data-toggle="modal" href="#Popup_EditArticle" ng-click="ShowEditArticle(x)" href="javascript:;" class="btn btn-circle btn-default btn-sm" title="@ONE_WEB.Resources.MyResources.GetResourceByKey("Edit", HttpContextAccessor.HttpContext?.User.FindFirst("Culture")?.Value)">
													<i class="fa fa-pencil"></i>
												</a>
